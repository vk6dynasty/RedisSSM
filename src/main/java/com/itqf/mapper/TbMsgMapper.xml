<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.itqf.mapper.TbMsgMapper">
    
    <select id="findAl" resultType="com.itqf.pojo.TbMsg">
        select * from tb_msg
    </select>
    <insert id="save" parameterType="com.itqf.pojo.TbMsg">
        insert into tb_msg(name,msg) value(#{name},#{msg});
    </insert>

</mapper>